<h1>Listing applicant_job_preferences</h1>

<table>
  <tr>
    <th>Prefer job time</th>
    <th>Prefer start available</th>
    <th>Prefer expected salary</th>
    <th>Applicant</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @applicant_job_preferences.each do |applicant_job_preference| %>
  <tr style="background-color: <%= cycle("#EBEBEB", "c5c5c5") %>">
    <td><%= applicant_job_preference.prefer_job_time %></td>
    <td><%= applicant_job_preference.prefer_start_available %></td>
    <td><%= applicant_job_preference.prefer_expected_salary %></td>
    <td><%= applicant_job_preference.applicant.applicant_name %></td>
    <td><%= applicant_job_preference.location.name %></td>
    <td><%= link_to 'Show', [applicant_job_preference.applicant,applicant_job_preference] %></td>
    <td><%= link_to 'Edit', edit_applicant_applicant_job_preference_path({:applicant_id => applicant_job_preference.applicant, :id => applicant_job_preference}) %></td>
    <td><%= link_to 'Destroy', applicant_job_preference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />


