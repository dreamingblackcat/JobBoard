<h1 class="text-center stitle">Listing applicant_job_preferences</h1>
<br/>
<table class="table table-bordered">
  <tr class="title">
    <th>job time type</th>
    <th>Available Start Date</th>
    <th>Expected Salary</th>
    <th>Applicant</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @applicant_job_preferences.each do |applicant_job_preference| %>
  <tr class="content">
    <td><%= applicant_job_preference.prefer_job_time %></td>
    <td><%= applicant_job_preference.prefer_start_available %></td>
    <td><%= applicant_job_preference.prefer_expected_salary %></td>
    <td><%= applicant_job_preference.applicant.applicant_name %></td>
    <td><%= applicant_job_preference.location.name %></td>
    <td><%= link_to 'Show', [applicant_job_preference.applicant.user,applicant_job_preference.applicant,applicant_job_preference] %></td>
    <td><%= link_to 'Edit', edit_user_applicant_applicant_job_preference_path({:user_id=>applicant_job_preference.applicant.user,:applicant_id => applicant_job_preference.applicant, :id => applicant_job_preference}) %></td>
    <td><%= link_to 'Destroy', applicant_job_preference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
<%= will_paginate @applicant_job_preferences, :renderer=> BootstrapPagination::Rails %>
<br />
<%= link_to 'New', new_user_applicant_applicant_job_preference_path(@user,@applicant), :class=>"btn btn-primary" %>
<%= render :partial=>"layouts/applicant_sidebars"%>


